    "version": "0.3.1",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "description": "",
    "exports": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
    },
    "scripts": {
        "build": "bun run format && bun run build.ts",
        "dev": "bun build ./src/index.ts --outdir ./dist --watch",
        "prepublishOnly": "bun run build",
        "test": "vitest",
        "format": "prettier --write ./src && prettier --write package.json"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "svelte",
        "writable",
        "store",
        "bun"
