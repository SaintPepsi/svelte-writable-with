    "author": "Ian Hogers <info@iahogers.com>",
    "devDependencies": {
        "@types/bun": "^1.1.11",
        "@types/eslint": "^9.6.1",
        "@types/lodash": "^4.17.10",
        "bun-plugin-dts": "^0.3.0",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "globals": "^15.11.0",
        "lodash": "^4.17.21",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.7",
